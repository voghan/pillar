<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:tags="[]"
            cq:template="/conf/pillar/settings/wcm/templates/xf-web-variation"
            cq:xfMasterVariation="{Boolean}true"
            cq:xfVariantType="web"
            jcr:primaryType="cq:PageContent"
            jcr:title="Header"
            sling:resourceType="pillar/components/xfpage/v1/xfpage">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="pillar/components/container/v1/container"
                layout="simple">
            <navigation
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="pillar/components/navigation/v1/navigation"
                    collectAllPages="false"
                    navigationRoot="/content/pillar/us/en"
                    skipNavigationRoot="false"
                    structureDepth="1"/>
            <languagenavigation
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="pillar/components/languagenavigation/v1/languagenavigation"
                    navigationRoot="/content/pillar/us"
                    structureDepth="2"/>
            <search
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="pillar/components/search/v1/search"
                    searchRoot="/content/pillar/us"/>
        </root>
    </jcr:content>
</jcr:root>
